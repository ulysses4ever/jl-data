# Population creation.

function popfull(n::Int, funcs::FuncSet, consts::ConstSet, vars::VarSet, maxdepth::Int)
    [randfull(funcs, consts, vars, maxdepth) for _=1:n]
end

function popgrow(n::Int, funcs::FuncSet, consts::ConstSet, vars::VarSet, maxdepth::Int)
    [randgrow(funcs, consts, vars, maxdepth) for _=1:n]
end

