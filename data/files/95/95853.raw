using JuMP
using InteriorPoint

println("BERTSIM EX")
m = Model(solver=InteriorPointSolver())
@defVar(m, x[1:6] >= 0)
@setObjective(m, Min, dot([-10,-12,-12,0,0,0], x))
@addConstraint(m, dot([1,2,2,1,0,0],x) == 20)
@addConstraint(m, dot([2,1,2,0,1,0],x) == 20)
@addConstraint(m, dot([2,2,1,0,0,1],x) == 20)
solve(m)

println("KNAPSACK")
m = Model(solver=InteriorPointSolver())
@defVar(m, x[1:5] >= 0)
@setObjective(m, Min, dot(-[ 5, 3, 2, 7, 4 ], x))
@addConstraint(m,     dot( [ 2, 8, 4, 2, 5 ], x) <= 10)
solve(m)