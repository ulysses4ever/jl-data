# ps3000CommonElements.jl

export PS3000TimeUnits, PS3000DriverErrorCodes

baremodule PS3000TimeUnits
  const FS = 0   # femptoseconds
  const PS = 1  # picoseconds
  const NS = 2  # nanoseconds
  const US = 3  # microseconds
  const MS = 4  # miliseconds
  const S = 5   # seconds
end # module

baremodule PS3000DriverErrorCodes
  const OK = 0
  const MAX_UNITS_OPENED = 1
  const MEM_FAIL = 2
  const NOT_FOUND = 3
  const FW_FAIL = 4
  const NOT_RESPONDING = 5
  const CONFIG_FAIL = 6
  const OS_NOT_SUPPORTED =7
end # module

