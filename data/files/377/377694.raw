#!/usr/bin/julia

println("Sorting and Collections")
println("-------------------------")

data = [4, 2, 1, 3, 5]

println("isEmpty:",isempty(data))

println("sorting collection")
ordered = sort(data)

println("original collection")
println(data)

println("new sorted collection")
println(ordered)

println("sorting collection in place")
sort!(data)

println("original collection")
println(data)

println("empty the collection")
empty!(data)

println("isEmpty:",isempty(data))
