###################################################
## Match tests
###################################################

g1 = DiGraph(4)
add_edge!(g1, 1,2)
add_edge!(g1, 2,1)
add_edge!(g1, 1,3)
add_edge!(g1, 3,4)
add_edge!(g1, 4,1)

w1 = [1.0,1.0,1.0,1.0]

m, v, t = match(g1, w1, techno = "2_cycles", solver = "gurobi")
@test m == [1,1,0,0]
@test v == 2

m, v, t = match(g1, w1, techno = "23_cycles", solver = "gurobi")
@test v == 3

add_edge!(g1, 2,3)
m, v, t = match(g1, w1, techno = "23_cycles", solver = "gurobi")
@test v == 3

w2 = [1.0,3.0,1.0,1.0]
m, v, t = match(g1, w2, techno = "23_cycles", solver = "gurobi")
@test m == [1,1,0,0]
@test v == 4

m, v, t = match(g1, w2, techno = "2_cycles", solver = "gurobi")
@test v == 4
