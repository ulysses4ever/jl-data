		(s4a, x4a, d4a) = simSUDC(Na, NCol)
		(s1b, x1b, d1b) = simSDV(Nb)
		(s2b, x2b, d2b) = simSUDV(Nb)
		(s3b, x3b, d3b) = simSDC(Nb, NCol)
		(s4b, x4b, d4b) = simSUDC(Nb, NCol)
