ation.search);\r\n            if(params.region == 'NI' || params.region == 'ni'){\r\n                $(\"div.contentRow:eq(1)\").show();\r\n            }\r\n            else if(params.region == 'ROI'){\r\n                $(\"div.contentRow:eq(0)\").show();\r\n            }\r\n            else if(params.region == 'GB'){\r\n                $(\"div.contentRow:eq(2)\").show();\r\n            }\r\n            else if(params.region == 'WW'){\r\n                $(\"div.contentRow:eq(3)\").show();            \r\n            }\r\n        }\r\n    </script>\r\n    \r\n    \r\n\r\n    <script src=\"../Scripts/ui.core.js\" type=\"text/javascript\"></script>\r\n\r\n    <script src=\"../Scripts/ui.datepicker.js\" type=\"text/javascript\"></script>\r\n\r\n    <script src=\"../Scripts/jquery.validate.min.js\" type=\"text/javascript\"></script>\r\n\r\n    <script type=\"text/javascript\">\r\n        $(function() {\r\n\t\t    $('#datepicker').datepicker({\r\n\t\t            dateFormat: 'DD, d MM, yy'\r\n\t\t        });\r\n\t\t        \r\n\t\t    $('#aspnetForm').validate({\r\n\t\t        rules: {\r\n\t\t            'ctl00$content$txtCompanyName': {required: true},\r\n\t\t            'ctl00$content$txtFirstName': {required: true},\r\n\t\t            'ctl00$content$txtLastName': {required: true},\r\n\t\t            'ctl00$content$txtEmail': {required : true, email: true},\r\n\t\t            'datepicker': {required: true, date: true},\r\n\t\t            'ctl00$content$txtContactTime': {required: true},\r\n\t\t            'ctl00$content$txtEstNoCons' : {required: true}\t\t            \r\n\t\t        },\r\n\t\t        messages: {\r\n\t\t            'ctl00$content$txtCompanyName': \"  Please enter your company name\",\r\n\t\t            'ctl00$content$txtFirstName': \"  Please enter your first name\",\r\n\t\t            'ctl00$content$txtLastName': \"  Please enter your last name\",\r\n\t\t            'ctl00$content$txtEmail': {required : \"  Please enter your email\", email: \"  Please enter a valid email\"},\r\n\t\t            'datepicker': {required: \"  Please enter a contact date\", date: \"  Please enter a valid date\"},\r\n\t\t            'ctl00$content$txtContactTime': \"  Please enter a contact time\",\r\n\t\t            'ctl00$content$txtEstNoCons' : \"  Please enter estimated no. of consignments\"\t\t            \t\t            \r\n\t\t        }\r\n\t\t    });\r\n\t\t    \r\n\t    });\r\n\t    \r\n\t    $.validator.setDefaults({\n\t        submitHandler: function() { \n\t            var serverElem = $('#ctl00_content_dateValue');\r\n\t            serverElem.val($('#datepicker').val());\r\n\t            \r\n\t            var mobile = $('#ctl00_content_txtWorkPhone');\r\n\t            var work = $('#ctl00_content_txtMobile');\r\n\t            \r\n\t            if(mobile.val() == '' && work.val() == ''){\r\n\t                alert(\"Please enter a phone number\");\r\n\t            }\r\n\t            else{\t            \n\t                form1.submit();\n\t            }\n\t        }\n        });\r\n\t    \r\n    </script>\r\n\r\n    <style type=\"text/css\">\r\n        .text { width: 195px; }\r\n    </style>\r\n\r\n\r\n<script type=\"text/javascript\">\r\n\r\n    var _gaq = _gaq || [];\r\n    _gaq.push(['_setAccount', 'UA-29618091-5']);\r\n    _gaq.push(['_trackPageview', '']);\r\n    (\r\n        function () {\r\n            var ga = document.createElement('script');\r\n            ga.type = 'text/javascript';\r\n            ga.async = true;\r\n            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';\r\n            var s = document.getElementsByTagName('script')[0];\r\n            s.parentNode.insertBefore(ga, s);\r\n        }\r\n    )();\r\n\r\n</script>\r\n\r\n</head>\r\n<body>\r\n    <form name=\"aspnetForm\" method=\"post\" action=\"ContactEnquiry.aspx\" id=\"aspnetForm\">\r\n<input type=\"hidden\" name=\"__VIEWSTATE\" id=\"__VIEWSTATE\" value=\"\" />\r\n\r\n\r\n<script