 department a faculty belongs to\r\nif (collegeIndex == 0)\r\n\t{\r\n\tdocument.write(\"<link href='http://www.ucc.ie/css/filedownload,150,en.css' rel='stylesheet' type='text/css'>\");\r\n\tcollegeName = \"College of Arts, Celtic Studies and Social Sciences\";\r\n\t}\r\n\r\nelse if (collegeIndex == 1)\r\n\t{\r\n\tdocument.write(\"<link href='http://www.ucc.ie/css/filedownload,154,en.css' rel='stylesheet' type='text/css'>\");\r\n\tcollegeName = \"College of Business and Law\";\r\n\t}\r\n\r\nelse if (collegeIndex == 2)\r\n\t{\r\n\tdocument.write(\"<link href='http://www.ucc.ie/css/filedownload,155,en.css' rel='stylesheet' type='text/css'>\");\r\n\r\n\tcollegeName = \"College of Medicine and Health\";\r\n\t}\r\n\r\nelse if (collegeIndex == 3)\r\n\t{\r\n\tdocument.write(\"<link href='http://www.ucc.ie/css/filedownload,153,en.css' rel='stylesheet' type='text/css'>\");\r\n\tcollegeName = \"College of Science, Engineering and Food Science\";\r\n\t}\r\n\r\nelse if (collegeIndex == 4)\r\n\t{\r\n\tdocument.write(\"<link href='http://www.ucc.ie/css/ccjhr.css' rel='stylesheet' type='text/css'>\");\r\n\tcollegeName = \"College of Business and Law\";\r\n\t}\r\n\r\n-->\r\n\r\n</script>\r\n<noscript>\r\n\t<link href='/en/styleassets/oldcss/filedownload,43,en.css' rel='stylesheet' type='text/css'/>\r\n</noscript>\r\n<link href='http://www.ucc.ie/css/print.css' rel=\"stylesheet\" type=\"text/css\" media=\"print\" />\r\n<!-- navigation object : analytics -->\r\n</head>\r\n<body>\r\n\r\n<div id=\"crest\">\r\n  <a href=\"/en/\"><img src=\"http://www.ucc.ie/en/images/filedownload,2,en.gif\" alt=\"UCC Logo - For Web Use Only. Please go to http://upic.ucc.ie for access to print quality versions\" title=\"UCC Logo - For Web Use Only. Please go to http://upic.ucc.ie for access to print quality versions\" width=\"200\" height=\"100\" border=\"0\" /></a>\r\n</div>\r\n\r\n<div id=\"search\">\r\n<form action=\"http://www.ucc.ie/cgi-bin/uncgi/Find\" method=\"post\">\r\n  <table width=\"200\" bord