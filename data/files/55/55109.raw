using Typeset
import Typeset.typeset
using Base.Test

function foo(x, y)
  x+y
end

function typeset(t::Typesettable{:<=})
  string(t.value[1], " \\leq ", t.value[2])
end

answer, tex = @typeset (1+(2+3)*4)/foo(5,16)

@test answer == 1

println(tex)

answer, tex = @typeset(2-1 <= 3)
println(tex)

answer, tex = @typeset (5+(2+3)*4)/5

println(answer, "//", tex)

