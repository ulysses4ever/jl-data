
d = SeqUtils.sim_trivial(1)
@test size(d) == (1, 3)
d = SeqUtils.sim_trivial(10)
@test size(d) == (10, 3)

d = SeqUtils.sim_basic(1)
@test size(d) == (1, 7)
d = SeqUtils.sim_basic(10)
@test size(d) == (10, 7)

d = SeqUtils.sim_redundant(1)
@test size(d) == (1, 6)
d = SeqUtils.sim_redundant(10)
@test size(d) == (10, 6)