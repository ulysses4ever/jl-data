#Problem 77 - Prime summations

################################################
function prime_sum()
  target = 5000
  partLen = 100
  ways = zeros(Int64,partLen+2)
  ways[1] = 0
  for i = 2:2:partLen
    ways[i] = 0
    ways[i+1] = 1
  end
  for i in primes(partLen)[2:end]
    ways[i+1] += 1
    for j = i+1:partLen
        ways[j+1] += ways[j-i+1]
    end

  end
  for (h_ind,h) in enumerate(ways)
    if h > target
      println(h, " ", h_ind-1)
      return
    end
  end
end
################################################
@time prime_sum()
