export example1, example2

function example1(n::Int)
    A = speye(n)
    A[n,1:end-1] = 0.2
    A[1:end-1, 5] = 0.2
    A
end
example1() = example1(8)

function example2()
    A = speye(9)*10
    I_up = [1,3,2,3,1,4,1,4,2,5,7,4]
    J_up = [4,4,5,5,6,6,7,7,8,8,8,9]
    I = cat(1, I_up, J_up)
    J = cat(1, J_up, I_up)
    V = ones(length(I))
    return A + sparse(I, J, V, 9, 9)
end
