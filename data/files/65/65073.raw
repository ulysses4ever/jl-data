export example1, example2, example3, example4

function example1(n::Int)
    A = speye(n)
    A[n,1:end-1] = 0.2
    A[1:end-1, 5] = 0.2
    A
end
example1() = example1(8)

function example2()
    A = speye(9)*10
    I_up = [1,3,2,3,1,4,1,4,2,5,7,4]
    J_up = [4,4,5,5,6,6,7,7,8,8,8,9]
    I = cat(1, I_up, J_up)
    J = cat(1, J_up, I_up)
    V = ones(length(I))
    return A + sparse(I, J, V, 9, 9)
end

# from the paper 
#"The Multifrontal Method for Sparse Matrix Solution: Theory and Practice"
# by Joseph W. H. Liu
function example3()
    A = speye(9)*10
    I_up = [2,3,2,5,1,1,3,4,5,7,1,4,6,7]
    J_up = [4,5,6,6,7,8,8,8,8,8,9,9,9,9]
    I = cat(1, I_up, J_up)
    J = cat(1, J_up, I_up)
    V = ones(length(I))
    return A + sparse(I, J, V, 9, 9)
end

# postordered sparse matrix of example3
function example4()
    A = speye(9)*10
    I_up = [1,3,5,3,6,1,2,4,5,6,1,2,4,7]
    J_up = [2,4,6,7,7,8,8,8,8,8,9,9,9,9]
    I = cat(1, I_up, J_up)
    J = cat(1, J_up, I_up)
    V = ones(length(I))
    return A + sparse(I, J, V, 9, 9)
end
