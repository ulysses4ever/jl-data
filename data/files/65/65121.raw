using Graphs


"convert a sparse matrix A to an adjacency graph G(A)"
function sp2adj(A::SparseMatrixCSC)
    n = A.n
    g = simple_adjlist(n, is_directed = false)
    for j = 1:n
        for k = A.colptr[j]:A.colptr[j+1]-1
            i = A.rowval[k]
            if i > j
                add_edge!(g,i,j)
            end
        end
    end
    g                   
end

