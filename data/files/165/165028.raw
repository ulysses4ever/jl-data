using Base.Test
rbc = Model(:[c+(k-(1-δ)*k[-1])-Y
			  ϕh*(1-h)^-σh/c^-σc-W
		      k-β*Expect(λ[+1]*R[+1]*k)/λ
],:[    k=(3,12,6)
        η = (1,0.9,0.01,2)
],:[    c = (0,10,.7),
        h = (0,1,0.35),
        k = (0,100,9.5)
],:[    λ = c^-σc
		Y = η*k[-1]^α*h^(1-α)
		W = (1-α)*k[-1]^α*η*h^-α
		R = (1-δ+α*η*(k[-1]/h)^(α-1))
],:[    α = 0.33
		δ = 0.025
		σc = 2.5
		ϕh = 1.66
		σh = 2
		β = 0.99])
solve(rbc)

@test abs(rbc.G(rbc[:k,0]-rbc[:k,-1],[9.575 1]))[1]<1e-3
