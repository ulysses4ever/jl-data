function utility1(cH::Float64, Q::Float64)

  # Declare utility parameters:
  aCH 		= 1/2
  aQH 		= 1/2
  gammaCH 	= 3/2
  gammaQH 	= 3/2


  utility = aCH*(cH^(1-gammaCH))/(1-gammaCH) + aQH*(Q^(1-gammaQH))/(1-gammaQH) 


end
