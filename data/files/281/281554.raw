function mUtil1_q(cH::Float64, Q::Float64)

  # Declare utility parameters:
  aCH 		= 1/2
  aQH 		= 1/2
  gammaCH 	= 3/2
  gammaQH 	= 3/2


  marginal_utility = aQH * (Q^(-gammaQH)) 


end
