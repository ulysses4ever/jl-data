function utility2(cW::Float64, Q::Float64)

  # Declare utility parameters:
  aCW 		= 1/2
  aQW 		= 1/2
  gammaCW 	= 3/2
  gammaQW 	= 3/2


  utility = aCW*(cW^(1-gammaCW))/(1-gammaCW) + aQW*(Q^(1-gammaQW))/(1-gammaQW)


end
