function mUtil2_q(cW::Float64, Q::Float64)

  # Declare utility parameters:
  aCW 		= 1/2
  aQW 		= 1/2
  gammaCW 	= 3/2
  gammaQW 	= 3/2


  marginal_utility = aQW * (Q^(-gammaQW))

end
