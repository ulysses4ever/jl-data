for i = 1:size(table17p6,1)
    alpha = table17p6[i,1]
    m = sind(alpha)^2
    for j = 2:size(table17p6,2)
        phid = 5*(j-2)
        phi = degrees2radians(phid)
        e = table17p6[i,j]
        @test_approx_eq_eps E(phi,m) e 1e-8
        @test_approx_eq_eps E(-phi,m) -e 1e-8
        if phid == 90
            @test_approx_eq_eps E(m) e 1e-8
            @test_approx_eq_eps ellipke(m)[2] e 1e-8
        end
    end
end

