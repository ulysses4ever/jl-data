# Project Euler 5

# Attempt 1

function checkDiv(n)
	for i in [12,14,15,16,18,20]
		if ifloor(mod(n, i)) != 0
			return false
		end
	end
	return true
end

function Run()
	step = 11*13*17*19
	i = step
	while true
		if checkDiv(i)
			return i
		end
		i += step
	end
end

