# Translate an RNA string in to an amino acid
#

require("src/seq.jl")
require("src/kmer.jl")
const aatab = getAAtable()
const aarev = reverseAAtable(aatab)

#genome = transcribe("ATGGCCATGGCCCCCAGAACTGAGATCAATAGTACCCGTATTAACGGGTGA")
#aa = "MA"
# genome = transcribe("")
# aa = "KEVFEPHYY"
# genome = transcribe("")
# aa="TVCLNCRL"

genome = readall("B_brevis.txt");
genome = transcribe(replace(genome,"\n",""));
aa="VKLFPWFNQY"

aalen = length(aa)
kaa = aalen*3
tmp = map(x->aarev[x],collect(aa))

pos = ASCIIString[]

for i in tmp[1]
	ea = eachmatch(Regex(i),genome,true)
	for ma in ea
		top = ma.offset
		starts = range(top+3,3,aalen-1)
		cc = 2
		failed = false
		for j in starts
			#println(j)
			if !failed
				#print(j)
				tmpcodon = genome[j:j+2]
				codons = aarev[aa[cc]]
				if !in(tmpcodon,codons)
					failed = true
				else
					cc += 1
				end
			end
		end
		if !failed
			push!(pos, reverse_transcribe(genome[top:(top+kaa-1)] ))
		end
	end
end


genome = transcribe(rc(reverse_transcribe(genome)));

for i in tmp[1]
	ea = eachmatch(Regex(i),genome,true)
	for ma in ea
		top = ma.offset
		starts = range(top+3,3,aalen-1)
		cc = 2
		failed = false
		for j in starts
			#println(j)
			if !failed
				#print(j)
				tmpcodon = genome[j:j+2]
				codons = aarev[aa[cc]]
				if !in(tmpcodon,codons)
					failed = true
				else
					cc += 1
				end
			end
		end
		if !failed
			println("Adding RC")
			push!(pos, rc(genome[top:(top+kaa-1)]) )
		end
	end
end

