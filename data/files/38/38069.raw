function beale()
  f(x) = (-1.5+x[1]*(1.0-x[2]))^2 + (-2.25+x[1]*(1.0-x[2]^2))^2 + (-2.625+x[1]*(1.0-x[2]^3))^2
  ∇f(x) = [2*(-1.5+x[1]*(1-x[2]))*(1-x[2])+2*(-2.25+x[1]*(1-x[2]^2))*(1-x[2]^2)+2*(-2.625+x[1]*(1-x[2]^3))*(1-x[2]^3);
           2*(-1.5+x[1]*(1-x[2]))*(-x[1])+2*(-2.25+x[1]*(1-x[2]^2))*(-2*x[1]*x[2])+2*(-2.625+x[1]*(1-x[2]^3))*(-3*x[1]*x[2]^2)]
  H(x) = [2*(1-x[2])^2+2*(1-x[2]^2)^2+2*(1-x[2]^3)^2 -2*(-1.5+x[1]*(1-x[2]))+2*(1-x[2])*(-x[1])-4*x[2]*(-2.25+x[1]*(1-x[2]^2))+2*(1-x[2]^2)*(-2*x[1]*x[2])-6*x[2]^2*(-2.625+x[1]*(1-x[2]^3))+2*(1-x[2]^3)*(-3*x[1]*x[2]^2);
          -2*(-1.5+x[1]*(1-x[2]))+2*(1-x[2])*(-x[1])-4*x[2]*(-2.25+x[1]*(1-x[2]^2))+2*(1-x[2]^2)*(-2*x[1]*x[2])-6*x[2]^2*(-2.625+x[1]*(1-x[2]^3))+2*(1-x[2]^3)*(-3*x[1]*x[2]^2) 2*x[1]^2-4*x[1]*(-2.25+x[1]*(1-x[2]^2))+8*x[1]^2*x[2]^2-12*x[1]*x[2]*(-2.625+x[1]*(1-x[2]^3))+18*x[1]^2*x[2]^4]
  x₀ = [1.0;1.0]
  return f, ∇f, H, x₀
end
