function humps()
  f(x) = 0.05*(x[1]^2+x[2]^2)+(sin(20*x[1])*sin(20*x[2]))^2
  ∇f(x) = [0.1*(x[1])+40*sin(20*x[1])*cos(20*x[1])*(sin(20*x[2]))^2;
          0.1*(x[2])+40*sin(20*x[2])*cos(20*x[2])*(sin(20*x[1]))^2]
  H(x) = [0.1+40*20*(sin(20*x[2]))^2*((cos(20*x[1]))^2-(sin(20*x[1]))^2) 40*40*sin(20*x[1])*cos(20*x[1])*sin(20*x[2])*cos(20*x[2]);
         40*40*sin(20*x[1])*cos(20*x[1])*sin(20*x[2])*cos(20*x[2]) 0.1+40*20*(sin(20*x[1]))^2*((cos(20*x[2]))^2-(sin(20*x[2]))^2)]
  x₀ = [-506.0;-506.2]
  return f, ∇f, H, x₀
end
