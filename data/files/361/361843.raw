using PyPlot
data_1 = readcsv("res/ble1.csv")
data_2 = readcsv("res/ble2.csv")
data_3 = readcsv("res/ble3.csv")
rssi_1 = convert(Array{Int8,1},data_1[:,2])
rssi_2 = convert(Array{Int8,1},data_2[:,2])
rssi_3 = convert(Array{Int8,1},data_3[:,2])
println("STD figure 1: ", std(rssi_1), "\nSTD figure 2: ", std(rssi_2), "\nSTD figure 3: ", std(rssi_3))
PyPlot.figure()
PyPlot.plt.hist(rssi_1, 20, normed="True")
PyPlot.figure()
PyPlot.plt.hist(rssi_2, 20, normed="True")
PyPlot.figure()
PyPlot.plt.hist(rssi_3, 20, normed="True")
