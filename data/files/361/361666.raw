module MyMergeSortTests

using FactCheck
using MyMergeSort


facts("inversion must be counted correctly") do
  @fact myMergeSort([1,3,5,2,4,6]) => ([1,2,3,4,5,6], 3)
  @fact myMergeSort([7,5,2,4,3,1,6]) => ([1,2,3,4,5,6,7], 14)
  @fact myMergeSort([5,1,4,2,2,3]) => ([1,2,2,3,4,5], 8)
  @fact myMerge([1,3,5],[2,4,6]) => ([1,2,3,4,5,6], 3)
end

end
