using Calendar

a = TimeStamp[]
b = TimeStamp[]

for i in 1:10
  newa = TimeStamp(p("2012-12-12")+days(i), i)
  push!(a, newa)
end

for i in 1:10
  newa = TimeStamp(p("2012-12-12")+days(i), i+.5)
  push!(b, newa)
end

c  =  [NaN, 0.248854, 0.808243, 0.380405, 0.528888, 0.714294, 0.689547, 0.414978, 0.671928, 0.953197, 0.0316208]

###################################################################
###### head, tail #################################################
###################################################################

a_head  = head(a)
a_tail  = tail(a)
a_first = first(a)
a_last  = last(a)

@assert 6  == a_head[6].value
@assert 10 == a_tail[6].value
@assert 1  == a_first[1].value
@assert 10 == a_last[1].value

