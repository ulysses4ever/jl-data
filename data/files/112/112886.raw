# Project Euler, problem 06
#   Find the difference between the sum of the squares of the first one hundred
#   natural numbers and the square of the sum.

function sumOfSquares(maxn::Int)
    nums = zeros(maxn)
    for i = 1:maxn
        nums[i] = i^2
    end
    return sum(nums)
end

squareOfSum(maxn::Int) = sum([1:maxn])^2

eul006(maxn::Int) = squareOfSum(maxn) - sumOfSquares(maxn)
