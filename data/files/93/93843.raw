# Players properties structure
type Player{N,UF}
  name::String 
  nodes::Array{N,1} 
  utilityfunctions::Array{UF,1}
end

# Nodes properties structure
type Node{M}
  name::String
  timeposistion::Int
  moves::Array{M,1}
end

# Moves properties structure
type Move{R,V}
  name::String
  length::Int
  childnodeindex::Nullable{Int}
  childnodeplayerindex::Nullable{Int}
  payoffset::Dict{R,V}
end
