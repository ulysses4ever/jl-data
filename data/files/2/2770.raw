export printRow, drawLine!, setPoint!, setPixel!, addRow!

@deprecate printRow(io, canvas, row) printrow(io, canvas, row)

@deprecate drawLine!(args...; nargs...) lines!(args...; nargs...)

@deprecate setPoint!(args...; nargs...) points!(args...; nargs...)

@deprecate setPixel!(args...; nargs...) pixel!(args...; nargs...)

@deprecate addRow!(args...; nargs...) addrow!(args...; nargs...)
