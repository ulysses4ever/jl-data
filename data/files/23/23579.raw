module SparseModelTest
using OnlineStats, FactCheck

facts("Constructors") do
    n, p = 1_000, 10
    x = randn(n, p)
    Î² = collect(1.:p)
    y = x*Î² + randn(n)
    o = StochasticModel(p, penalty = L1Penalty(.1))
    sp = SparseModel(o)
    update!(sp, x, y)

    @fact nobs(sp) --> 1_000
    @fact state(sp) --> state(sp.o)
    @fact statenames(sp) --> statenames(sp.o)

    HardThreshold(burnin = 123)
end

end
