function insertionSort!(arr::Array{Real})
  key::Integer = 0
  for j = 2:length(a)
    key = arr[j]

    i = j-1
    while i > 0 && arr[i] > key
      arr[i+1] = arr[i]
      i = i-1
    end

    arr[i+1] = key
  end
end

function mergeArrays(arr1, arr2)
  l1 = length(arr1)
  l2 = length(arr2)

  arr3 = zeros(l1 + l2)

  i = 1
  j = 1

  for k = 1:(l1+l2)
    if i > l1
      arr3[k] = arr2[j]
      j = j + 1
      continue
    end

    if j > l2
      arr3[k] = arr1[i]
      i = i + 1
      continue
    end

    if arr1[i] <= arr2[j]
      arr3[k] = arr1[i]
      i = i + 1
    else
      arr3[k] = arr2[j]
      j = j + 1
    end
  end

  arr3
end

function mergeSort(arr)
  if length(arr) > 1
    mid = floor(length(arr)/2)

    left  = mergeSort(arr[1:mid])
    right = mergeSort(arr[(mid+1):end])
    mergeArrays(left,right)
  else
    arr
  end
end