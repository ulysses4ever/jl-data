module Funciones

export CH
CH(x)=exp(-x).*(x.^2-2*x+1)

export GL
GL(x)=exp(-(x-5).^3)+x

export R
R(x)=(x.^2).*(exp(-x)).*sin(1./x.^2)
#Tener cuidado con el dominio

export POL
POL(x)=x.^4-pi*x.^3+3*x.^2-4*x+5

export ER
ER(x)=sin(1./(1-x).^2).*sqrt(x)

export J
J(x)=sqrt(107)*(-log(x))*x.^2

export ID
ID(x)=x

export CON
CON(x)=5

export PL
PL(x)=x.^9

end

