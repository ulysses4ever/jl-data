module Funciones

export CTE, ID, CH, GL, R, POL, ER, J, PL

CTE(X) = 5
ID(X) = X
CH(X) = exp(-X)*(X^2-2*X+1)
GL(X) = exp(-(X-5)^3) + X
R(X) = (X^2)*(exp(-X))*sin(1/X^2) #Tener cuidado con el dominio
POL(X) = X^4 - pi*X^3 + 3*X^2 - 4*X + 5
ER(X) = sin(1/(1-X)^2)*X^(1/2)
J(X) = sqrt(107)*(-log(X))*X^2

end
